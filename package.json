{
  "name": "rotmg-active-players",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{ts,tsx,css,mjs,json,yml,yaml}\" --write",
    "format:check": "prettier \"**/*.{ts,tsx,css,mjs,json,yml,yaml}\" --check",
    "aggregate": "bun run scripts/aggregate.ts",
    "migrate:realmeye": "bun run scripts/migrate-realmeye.ts",
    "migrate:realmstock": "bun run scripts/migrate-realmstock.ts",
    "migrate:launcher": "bun run scripts/migrate-launcher.ts",
    "scrape": "bun run scripts/scrape.ts",
    "scrape:dry-run": "bun run scripts/scrape.ts --dry-run",
    "baseline:prepare": "bun run scripts/prepare-baseline.ts",
    "baseline:verify": "bun run scripts/verify-growth.ts"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.542.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "uplot": "^1.6.31"
  },
  "devDependencies": {
    "@eslint/js": "^9",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.3.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
